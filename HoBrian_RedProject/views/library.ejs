<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Library Challenge</title>
    <link rel="stylesheet" href="/style2.css"> 
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
</head>
<body>
    <div class="library-background">
        <h1>Welcome To The Library of Babel</h1>
        <p>You can find anything if you know where you're looking...</p>
        <div class="hexagon-container">
        Hexagon: 
        </div>
        Wall: 1
        Shelf: 4
        Volume: 22
        Page: 386
        <br>
        <form id="flagForm">
            <input type="text" name="flag" placeholder="Enter Flag">
            <button type="submit">Submit Flag</button>
        </form>
    </div>
    <div class="message-box" id="messageBox"></div>
<script>
    document.getElementById('flagForm').addEventListener('submit', function (e) {
        e.preventDefault();

        const formData = new FormData(this);
        const flag = formData.get('flag');

        axios.post('/submit-flag', {
            flag: flag
        })
        .then(function (response) {
            showMessage(response.data);
        })
        .catch(function (error) {
            console.error('Error:', error);
        });
    });

    function showMessage(result) {
        messageBox.textContent = result.message;
        messageBox.style.display = 'block';

        if (result.correct) {
            messageBox.className = 'message-box success';
            // Wait for 3 seconds before redirecting
            setTimeout(function() {
                window.location.href = '/infinitelinks';
            }, 3000);
        }
        else {
            messageBox.className = 'message-box failure';
        }
    }
</script>
</body>
</html>